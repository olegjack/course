							jDesc.css({ "left": (iFullW - iNewW - iPadding - iDescW) + "px" });
						}
						jDesc.width(iDescW);
						jDescText.width(iDescW - 2*iDescPadding);
						break;
					}
					case "bottom":
					{
						jDesc.width(iFullW - 2*iBW).css({ "left": 0, "bottom": 0 });
						jDescText.width(iFullW - 2*iBW - 2*iDescPadding);
						switch(sScaling)
						{
							case "w":
							{
								iNewW = iFullW;
								nRatio = iNewW/iW;
								iNewH = Math.round(iH*nRatio);
								jImg.width(iNewW).height(iNewH);
								jContainer.height((iNewH>iFullH) ? iFullH : iNewH).css({ "top": 0, "left": 0 });
								break;
							}
							case "none":
							{
								iNewW = iW;
								iNewH = iH;
								jImg.width(iNewW).height(iNewH);
								jContainer.height((iNewH>iFullH) ? iFullH : iNewH).css({ "top": 0, "left": 0 });
								break;
							}
							case "hw":
							{
								iNewW = Math.round(0.5*iFullW);
								nRatio = iNewW/iW;
								iNewH = Math.round(iH*nRatio);
								jImg.width(iNewW).height(iNewH);
								jContainer.height((iNewH>iFullH) ? iFullH : iNewH).css({ "top": 0, "left": 0 });
								break;
							}
							case "hh":
							default:
							{
								iNewH = Math.round(0.5*iFullH);
								nRatio = iNewH/iH;
								iNewW = Math.round(iW*nRatio);
								jImg.width(iNewW).height(iNewH);
								jContainer.height(iNewH).css({ "top": 0, "left": 0 });
								break;
							}
						}
						iDescH = iFullH - iNewH - iPadding;
						if(iDescH<100)
						{
							iDescH = 100;
							jDesc.css({ "bottom": (iFullH - iNewH - iPadding - iDescH) + "px" });
						}
						jDesc.height(iDescH);
						jDescText.height(iDescH - 2*iDescPadding);
						break;
					}
					case "right":
					{
						jDesc.height(iFullH - 2*iBW).css({ "right": 0, "top": 0 });
						jDescText.height(iFullH - 2*iBW - 2*iDescPadding);
						switch(sScaling)
						{
							case "none":
							{
								iNewW = iW;
								jImg.width(iNewW).height(iH);
								jContainer.width(iNewW).css({ "top": 0, "left": 0 });
								break;
							}
							case "hw":
							{
								iNewW = 0.5*iFullW;
								nRatio = iNewW/iW;
								iNewH = Math.round(iH*nRatio);
								jImg.width(iNewW).height(iNewH).css({ "margin-top": Math.round(0.5*(iFullH - iNewH)) + "px" });
								jContainer.width(iNewW).css({ "top": 0, "left": 0 });
								break;
							}
							case "hh":
							{
								iNewH = Math.round(0.5*iFullH);
								nRatio = iNewH/iH;
								iNewW = Math.round(iW*nRatio);
								jImg.width(iNewW).height(iNewH).css({ "margin-top": Math.round(0.25*iFullH) + "px" });
								jContainer.width(iNewW).css({ "top": 0, "left": 0 });
								break;
							}
							case "h":
							default:
							{
								iNewH = iFullH;
								nRatio = iNewH/iH;
								iNewW = Math.round(iW*nRatio);
								jImg.width(iNewW).height(iNewH);
								jContainer.width(iNewW).css({ "top": 0, "left": 0 });
								break;
							}
						}
						iDescW = iFullW - iNewW - iPadding;
						if(iDescW<100)
						{
							iDescW = 100;
							jDesc.css({ "right": (iFullW - iNewW - iPadding - iDescW) + "px" });
						}
						jDesc.width(iDescW);
						jDescText.width(iDescW - 2*iDescPadding);
						break;
