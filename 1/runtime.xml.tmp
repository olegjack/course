<method type="v_rapid_008" name="Display">
<![CDATA[function Display(argobj)
			{
				var oCurObj = CLO[argobj.pid];
				if(oCurObj==null) return false;
				if(oCurObj.div==null) return false;
				var jCurObjDiv = $(oCurObj.div);
				var iIndex = 1;
				if(argobj.elindex!=null)
				{
					iIndex = parseInt(argobj.elindex, 10);
					if(isNaN(iIndex)) return false;
				}
				else
				{
					if(argobj.elem!=null)
					{
						if($(argobj.elem).hasClass("cl-header-btn-right"))
						{
							iIndex = oCurObj.data.iCurrent + 1;
						}
						else if($(argobj.elem).hasClass("cl-header-btn-left"))
						{
							iIndex = oCurObj.data.iCurrent - 1;
						}
					}
				}
				if(iIndex<1 || iIndex>oCurObj.data.iMax) return false;
				var jElement = $(oCurObj.jxParams.find("elements > item")[iIndex-1]);
				var sObjectId = jElement.children("objectid").text();
				var iPointX;
				var iPointY;
				var oPointOffset;
				if(CLO[sObjectId]==null)
				{
					var jObjectNode = $(CL.jxModule).find("object[id='" + sObjectId + "']");
					if(jObjectNode.length==0) return false;
					var iX = +jObjectNode.attr("x");
					var iY = +jObjectNode.attr("y");
					var iW = +jObjectNode.attr("w");
					var iH = +jObjectNode.attr("h");
					if(iW>iH)
					{
						iPointX = iPointY = Math.floor(0.5*iH);
					}
					else
					{
						iPointX = iPointY = Math.floor(0.5*iW);
					}
					oPointOffset = { left: iX, top: iY };
				}
				else
				{
					oPointOffset = $(CLO[sObjectId].div).offset();
					iPointX = +CLO[sObjectId].data.nOffsetX;
					iPointY = +CLO[sObjectId].data.nOffsetY;
				}
				var oContentOffset = jCurObjDiv.find(".cl-tab-block").offset();
				var jPointer = jCurObjDiv.find(".cl-pointer");
				var jPointerBgArrow = jPointer.find(".cl-pointer-bg-arrow");
				var jPointerArrow = jPointer.find(".cl-pointer-arrow");
				var jPointerLine = jPointer.find(".cl-pointer-line");
				var jDesc = jCurObjDiv.find(".cl-desc");
				var iPointLeft = Math.round((oPointOffset.left - oContentOffset.left)/CLZ.nZoom + iPointX); // jquery offset/position zoom bug
				var iPointTop = Math.round((oPointOffset.top - oContentOffset.top)/CLZ.nZoom + iPointY); // jquery offset/position zoom bug
				jPointer.hide();
				switch(oCurObj.jxParams.find("desc_position").text())
				{
					case "left":
					{
						var iDescRightX = Math.round(jDesc.position().left/CLZ.nZoom + jDesc.outerWidth()); // jquery offset/position zoom bug
						var iArrowW = jPointerBgArrow.outerWidth();
						var iPointerW = iPointLeft - iDescRightX;
						if(iPointerW > iArrowW)
						{
							var iPointerLineW = Math.round(iPointerW - jPointerArrow.outerWidth());
							jPointer.width(iPointerW).css({ "left": iDescRightX + "px", "top": Math.round(iPointTop - 0.5*jPointerBgArrow.outerHeight()) + "px" });
							jPointerLine.width(iPointerLineW);
							jPointer.show("blind", { "direction": "left" });
						}
						break;
					}
					case "right":
					{
						var iDescLeftX = Math.round(jDesc.position().left/CLZ.nZoom); // jquery offset/position zoom bug
						var iArrowW = jPointerBgArrow.outerWidth();
						var iPointerW = iDescLeftX - iPointLeft;
						if(iPointerW > iArrowW)
						{

							var iPointerLineW = Math.round(iPointerW - jPointerArrow.outerWidth());
							jPointer.width(iPointerW).css({ "left": iPointLeft + "px", "top": Math.round(iPointTop - 0.5*jPointerBgArrow.outerHeight()) + "px" });
							jPointerLine.width(iPointerLineW);
							jPointer.show("blind", { "direction": "right" });
						}
						break;
					}
					case "bottom":
					{
						var iDescTopY = Math.round(jDesc.position().top/CLZ.nZoom); // jquery offset/position zoom bug
						var iArrowH = jPointerBgArrow.outerHeight();
						var iPointerH = iDescTopY - iPointTop;
						if(iPointerH > iArrowH)
						{
							var iPointerLineH = Math.round(iPointerH - jPointerArrow.outerHeight());
							jPointer.height(iPointerH).css({ "left": Math.round(iPointLeft - 0.5*jPointerBgArrow.outerWidth()) + "px", "top": iPointTop + "px" });
							jPointerLine.height(iPointerLineH);
							jPointer.show("blind", { "direction": "down" });
						}
						break;
					}
				}
				jCurObjDiv.find(".cl-desc-text").hide();
				jCurObjDiv.find("#" + argobj.pid + "_DESC_TEXT_" + iIndex).fadeIn();
				oCurObj.FireEvent({ sName: "onDisplayItem" });
				if(iIndex==1 && oCurObj.data.iTotalElems>1)
				{
					oCurObj.FireEvent({ sName: "onDisplayFirstItem" });
				}
				else if(iIndex==oCurObj.data.iTotalElems && oCurObj.data.iTotalElems>1)
				{
					oCurObj.FireEvent({ sName: "onDisplayLastItem" });
				}
				var iIdx = $.inArray(iIndex, CLZ.oStore[argobj.pid]);
				if(iIdx==-1)
				{
					CLZ.oStore[argobj.pid].push(iIndex);
					if(CLZ.oStore[argobj.pid].length>=oCurObj.data.iTotalElems)
					{
						oCurObj.FireEvent({ sName: "onAllVisited" });
					}
				}
				else
				{
					CLZ.oStore[argobj.pid].splice(iIdx, 1);
					CLZ.oStore[argobj.pid].push(iIndex);
				}
				CL.Sound.Stop({ sId: argobj.pid, sEvent: "narration" });
				var sNarrationFile = $(oCurObj.jxParams.find("elements > item")[iIndex-1]).children("narration").text();
				if(sNarrationFile!="")
				{
					CL.Sound.Play({ sId: argobj.pid + "_" + argobj.iLevel, sEvent: "narration", sType: "custom", sFile: sNarrationFile });
				}
				else
				{
					CL.Sound.Stop({ sId: argobj.pid, sEvent: "narration" });
				}
				oCurObj.data.iCurrent = iIndex;
				oCurObj.CallMethod({ sMethod: "UpdateButtons" });
				return true;
			}]]>
</method>
<method type="v_rapid_008" name="GetProperty">
<![CDATA[function GetProperty(argobj)
			{
				var oCurObj = CLO[argobj.pid];
				if(oCurObj.div==null) return false;
