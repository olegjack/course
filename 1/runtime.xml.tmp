					case "left":
					{
						jDesc.height(iFullH - 2*iBW).css({ "left": 0, "top": 0 });
						jDescText.height(iFullH - 2*iBW - 2*iDescPadding);
						switch(sScaling)
						{
							case "none":
							{
								iNewW = iW;
								jImg.width(iNewW).height(iH);
								jContainer.width(iNewW).css({ "top": 0, "right": 0 });
